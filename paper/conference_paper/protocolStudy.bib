@article{Timmermans1997,
	Abstract = {In this paper, we argue that universality is always `local universality'. The achievement of local universality depends on how standards manage the tension involved in transforming work practices, while simultaneously being grounded in those practices. We investigate how this is done in two case studies -- an oncology protocol and the Cardio Pulmonary Resuscitation (CPR) protocol. These protocols are viewed as technoscientific scripts which crystallize multiple trajectories. In the process of obtaining local universality, we illustrate how protocols feed off previous standards and practices. We then indicate how the protocols function through the distributed work of a multitude of heterogeneous actors. Finally, we argue that, in this process, the protocols themselves are necessarily changed and partially reappropriated.},
	Author = {Timmermans, S. and Berg, M.},
	Date-Modified = {2015-05-15 18:52:23 +0000},
	Journal = {Social Studies of Science},
	Number = {2},
	Pages = {273--305},
	Title = {Standardization in Action: Achieving Local Universality through Medical Protocols},
	Volume = {27},
	Year = {1997},
	Bdsk-Url-1 = {http://dx.doi.org/10.1177/030631297027002003}}

@article{Sarini2004,
	Abstract = {The paper presents an approach and a technology to support the work$\backslash$nof biologists in their laboratories. By means of analysis of related$\backslash$nliterature, informal discussions with people working in laboratory,$\backslash$nand experience of one the authors with laboratory work in genomic$\backslash$nresearch, we identified different biologistsвЂ{\texttrademark} information needs$\backslash$nand different strategies in order to satisfy them. A relevant part$\backslash$nof biologistsвЂ{\texttrademark} work practice is about the continuous annotation$\backslash$nof their work in their personal notebooks. We claim that observing$\backslash$nnotebooks can boost a technology which is a generalization of recommendation$\backslash$nsystems and provides biologists with suggestions concerning their$\backslash$nwork. These suggestions are generated by combining the observation$\backslash$nof biologists performed actions in the past with actions patterns$\backslash$nincorporating biologistsвЂ{\texttrademark} work practices that are discovered by$\backslash$nthe system out of the transcriptions of biologistsвЂ{\texttrademark} notebooks.},
